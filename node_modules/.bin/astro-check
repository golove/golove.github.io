#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/golove/Documents/goloveBlog/node_modules/.pnpm/@astrojs+check@0.9.6_prettier@3.7.4_typescript@5.9.3/node_modules/@astrojs/check/bin/node_modules:/Users/golove/Documents/goloveBlog/node_modules/.pnpm/@astrojs+check@0.9.6_prettier@3.7.4_typescript@5.9.3/node_modules/@astrojs/check/node_modules:/Users/golove/Documents/goloveBlog/node_modules/.pnpm/@astrojs+check@0.9.6_prettier@3.7.4_typescript@5.9.3/node_modules/@astrojs/node_modules:/Users/golove/Documents/goloveBlog/node_modules/.pnpm/@astrojs+check@0.9.6_prettier@3.7.4_typescript@5.9.3/node_modules:/Users/golove/Documents/goloveBlog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/golove/Documents/goloveBlog/node_modules/.pnpm/@astrojs+check@0.9.6_prettier@3.7.4_typescript@5.9.3/node_modules/@astrojs/check/bin/node_modules:/Users/golove/Documents/goloveBlog/node_modules/.pnpm/@astrojs+check@0.9.6_prettier@3.7.4_typescript@5.9.3/node_modules/@astrojs/check/node_modules:/Users/golove/Documents/goloveBlog/node_modules/.pnpm/@astrojs+check@0.9.6_prettier@3.7.4_typescript@5.9.3/node_modules/@astrojs/node_modules:/Users/golove/Documents/goloveBlog/node_modules/.pnpm/@astrojs+check@0.9.6_prettier@3.7.4_typescript@5.9.3/node_modules:/Users/golove/Documents/goloveBlog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@astrojs/check/bin/astro-check.js" "$@"
else
  exec node  "$basedir/../@astrojs/check/bin/astro-check.js" "$@"
fi
